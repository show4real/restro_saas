{
	"name": "ipunkt/laravel-analytics",
	"type": "library",
	"description": "Analytics tracking for Laravel",
	"keywords": ["analytics", "google analytics", "laravel", "statistics", "javascript", "php"],
	"license": "MIT",
	"authors": [
		{
			"name": "Robert Kummer",
			"email": "r.kummer@ipunkt.biz"
		}
	],
	"require": {
		"php": "^7.2 || ^8.0",
		"illuminate/config": "~5.0 || ^6.0 || ^7.0 || ^8.0",
		"illuminate/session": "~5.0 || ^6.0 || ^7.0 || ^8.0",
		"illuminate/support": "~5.0 || ^6.0 || ^7.0 || ^8.0"
	},
	"require-dev": {
		"laravel/framework": "~5.0 || ^6.0 || ^7.0 || ^8.0"
	},
	"autoload": {
		"psr-0": {
			"Ipunkt\\LaravelAnalytics\\": "src/"
		}
	},
	"minimum-stability": "stable",
	"suggest": {
		"martijnc/php-csp": "For handling Content Security Policy matters"
	},
	"support": {
		"issues": "https://github.com/ipunkt/laravel-analytics/issues",
		"source": "https://github.com/ipunkt/laravel-analytics/archive/master.zip"
	},
	"extra": {
		"laravel": {
			"providers": [
				"Ipunkt\\LaravelAnalytics\\AnalyticsServiceProvider"
			],
			"aliases": {
				"Analytics": "Ipunkt\\LaravelAnalytics\\AnalyticsFacade"
			}
		}
	}
}
